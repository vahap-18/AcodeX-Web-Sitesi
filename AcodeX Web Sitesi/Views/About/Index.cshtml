<!--
Hakkımda sayfası
-->
@model List<About>
@using EntityLayer.Concrate
@{
    ViewData["Title"] = "Index";
}
<!doctype html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Online Study - Education Category Responsive Web Template - About : W3Layouts</title>
    <!-- google-fonts -->
    <link href="//fonts.googleapis.com/css2?family=Roboto:wght@100;300;400;500;700;900&display=swap"
          rel="stylesheet">
    <!-- //google-fonts -->
    <!-- Template CSS Style link -->
    <link href="~/tema/assets/css/style-starter.css" rel="stylesheet" />

    <!-- Favicons -->
    <link href="~/tema/blogtema/assets/img/favicon.png" rel="icon">
    <link href="~/tema/blogtema/assets/img/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Raleway:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i"
          rel="stylesheet">

    <!-- Vendor CSS Files -->
    <link href="~/tema/blogtema/assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/tema/blogtema/assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="~/tema/blogtema/assets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
    <link href="~/tema/blogtema/assets/vendor/glightbox/css/glightbox.min.css" rel="stylesheet">
    <link href="~/tema/blogtema/assets/vendor/remixicon/remixicon.css" rel="stylesheet">
    <link href="~/tema/blogtema/assets/vendor/swiper/swiper-bundle.min.css" rel="stylesheet">

    <!-- Template Main CSS File -->
    <link href="~/tema/blogtema/assets/css/style.css" rel="stylesheet" />
    <link href="~/tema/assets/css/style-starter.css" rel="stylesheet" />
</head>

<body>

    @await Html.PartialAsync("Header")
    <!-- Sayfa üst yol-->
    <div class="inner-banner">
        <section class="w3l-breadcrumb">
            <div class="container">
                <h4 class="inner-text-title font-weight-bold mb-sm-3 mb-2">About Us</h4>
                <ul class="breadcrumbs-custom-path">
                    <li><a class="nav-link" asp-area="" asp-controller="Home" asp-action="Index">Anasayfa</a></li>
                    <li class="active"><span class="fa fa-chevron-right mx-2" aria-hidden="true"></span>Hakkımda</li>
                </ul>
            </div>
        </section>
    </div>
    <!--//Sayfa üst yol-->

    <div class="blog_area blog-details-area rs pt-115 pb-90" id="blog">
        <div class="container">
            <div class="row">
               
                    <div class="col-lg-8 col-md-12 col-sm-12 col-xs-12">
                        <!-- Sayfa sol tarafı-->
                        <div class="col-md-12">
                            <div class="dreamit-single-blog-box">
                            @foreach (var item in Model)
                            {
                                <div class="img-container">
                                    <img src="~/tema/assets/images/blog1.jpg" style="max-height: 400px; width: 100%; object-fit: cover; margin-bottom: 50px;" />
                                </div>

                                <div class="gen em-blog-content" style="margin-top: 70px;">
                                    <div class="meta-blog">
                                        <h2>Benim hakkımda</h2>
                                    </div>

                                    <div class="blog-page-title pt-3">
                                        <p>@item.Detail</p>
                                    </div>
                                </div>
                            }
                            </div>

                        </div>
                    </div> <br />
                
                <!--SAYFA SAĞ TARAF-->
                <div class="col-lg-4 col-md-12 col-sm-12 col-xs-12 sidebar-right content-widget pdsr">
                    <!-- Yazarın Sosyal Meyadsı-->
                    @await Html.PartialAsync("../About/SocialMediaAbout")
                    <!-- //Yazarın Sosyal Meyadsı-->
                    <!-- ARŞİV-->
                    <div class="widget-sidebar-box-two" style="margin-bottom: 10px;  border-bottom:1px groove gray">
                        <div class="widget-sidebar-title text-left pb-20">
                            <h6>ARŞİV</h6>
                            <div class="em_bar_bg text-left1"></div>
                        </div>
                        <div class="categories-content">
                            <ul>
                                <li><a href="blog-details.html">February 2021</a></li>
                                <li><a href="blog-details.html">January 2021</a></li>
                            </ul>
                        </div><br />
                    </div>
                    <!-- //ARŞİV-->
                    <!--ABONE OL-->
                    @await Html.PartialAsync("../NewsLetter/SubscribeMail")
                    <!--//ABONE OL-->
                </div>

            </div>
        </div>
    </div>

    <!-- Yazarın Becerileri-->
    <div class="interests container">
        <div class="section-title">
            <h2>BECERİLERİM</h2>
        </div>
        <div class="row">
            <div class="col-lg-3 col-md-4 mt-4 mt-md-0">
                <div class="icon-box">
                    <i class="ri-microsoft-fill" style="color: #ffbb2c;"></i>
                    <h3>Nesne Tabanlı Programlama (OOP)</h3>
                </div>
            </div>
            <div class="col-lg-3 col-md-4 mt-4 mt-md-0">
                <div class="icon-box">
                    <i class="ri-bar-chart-box-line" style="color: #5578ff;"></i>
                    <h3>Entitiy Framework</h3>
                </div>
            </div>
            <div class="col-lg-3 col-md-4 mt-4 mt-md-0">
                <div class="icon-box">
                    <i class="ri-codepen-fill" style="color: #e80368;"></i>
                    <h3>Model View Controller (MVC)</h3>
                </div>
            </div>
            <div class="col-lg-3 col-md-4 mt-4 mt-lg-0">
                <div class="icon-box">
                    <i class="ri-code-fill" style="color: #e361ff;"></i>
                    <h3>ADO.NET</h3>
                </div>
            </div>
            <div class="col-lg-3 col-md-4 mt-4">
                <div class="icon-box">
                    <i class="ri-gallery-view-2" style="color: #47aeff;"></i>
                    <h3>Canva</h3>
                </div>
            </div>
            <div class="col-lg-3 col-md-4 mt-4">
                <div class="icon-box">
                    <i class="ri-window-fill" style="color: #47aeff;"></i>
                    <h3>C# Form</h3>
                </div>
            </div>
            <div class="col-lg-3 col-md-4 mt-4">
                <div class="icon-box">
                    <i class="ri-safe-fill" style="color: #47aeff;"></i>
                    <h3>Siber Güvenlik</h3>
                </div>
            </div>
            <div class="col-lg-3 col-md-4 mt-4">
                <div class="icon-box">
                    <i class="ri-terminal-box-fill" style="color: #47aeff;"></i>
                    <h3>Kali Linux</h3>
                </div>
            </div>
        </div>
        <div class="col-lg-3 col-md-4 mt-4">
            <div class="icon-box">
                <i class="ri-server-fill" style="color: #47aeff;"></i>
                <h3>Veri Tabanı</h3>
            </div>
        </div>
        <br />
    </div>
    <!--//Yazarın Becerileri-->
    <!-- Yazar hakkında yorumlar -->
    @*     Sonradan eklenecek geri bildirim sayfası
    @await Html.PartialAsync("../Comment/GeriBildirimYazar");
    *@
    <br />
    <section class="w3l-companies-hny-6 position-relative">
        <div class="cusrtomer-layout py-5">
            <div class="container py-md-4 py-3">
                <div class="title-heading-w3 text-center mx-auto">
                    <h3 class="title-big">GERİ BİLDİRİMLER</h3>
                </div>

                <div id="owl-demo1" class="owl-carousel owl-theme mt-5">
                    <div class="item">
                        <div class="testimonial-content">
                            <div class="testimonial">
                                <div class="testi-des">
                                    <div class="test-img">
                                        <img src="~/tema/blogtema/assets/images/testi2.jpg" class="img-fluid" alt="/">
                                    </div>
                                    <div class="peopl">
                                        <h3>Suzan Suzi</h3>
                                        <p class="indentity">Türkiye</p>
                                    </div>
                                </div>
                                <blockquote>
                                    <p>
                                        Sed ut perspiciatis unde omnis iste natus error sit voluptatem accusantium
                                        doloremque laudantium, totam rem aperiam.
                                    </p>
                                </blockquote>
                                <quote></quote>
                            </div>
                        </div>
                    </div>

                </div>
            </div>

        </div>
    </section>
    <!--//Yazar hakkında yorumlar -->


    <script src="~/tema/blogtema/assets/vendor/purecounter/purecounter_vanilla.js"></script>
    <script src="~/tema/blogtema/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/tema/blogtema/assets/vendor/glightbox/js/glightbox.min.js"></script>
    <script src="~/tema/blogtema/assets/vendor/isotope-layout/isotope.pkgd.min.js"></script>
    <script src="~/tema/blogtema/assets/vendor/swiper/swiper-bundle.min.js"></script>
    <script src="~/tema/blogtema/assets/vendor/waypoints/noframework.waypoints.js"></script>
    <script src="~/tema/blogtema/assets/vendor/php-email-form/validate.js"></script>

    <!-- Template Main JS File -->
    <script src="assets/js/main.js"></script>
</body>

</html>